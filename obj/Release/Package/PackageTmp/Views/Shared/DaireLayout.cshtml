
@{
    Layout = null;
}

<!doctype html>
<html class="no-js" lang="zxx">

<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Daire Yönetim</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- <link rel="manifest" href="/Content/DaireTemp/site.webmanifest"> -->
    <!-- Favicon icon -->
    <link rel="icon" href="~/Theme/files/assets/images/favs.png" type="image/png">
    <!-- Place favicon.ico in the root directory -->
    <!-- CSS here -->
    <link rel="stylesheet" href="/Content/DaireTemp/css/bootstrap.min.css">
    <link rel="stylesheet" href="/Content/DaireTemp/css/owl.carousel.min.css">
    <link rel="stylesheet" href="/Content/DaireTemp/css/magnific-popup.css">
    <link rel="stylesheet" href="/Content/DaireTemp/css/font-awesome.min.css">
    <link rel="stylesheet" href="/Content/DaireTemp/css/themify-icons.css">
    <link rel="stylesheet" href="/Content/DaireTemp/css/nice-select.css">
    <link rel="stylesheet" href="/Content/DaireTemp/css/flaticon.css">
